<!DOCTYPE html>
<html>
<head>
	<script type="text/javascript">
		window.OLSKPublicConstants = function (inputData) {
			return <%- JSON.stringify({
				OLSKSharedActiveRouteConstant,
				OLSKSharedPageCurrentLanguage,
				OLSKSharedPageLanguagesAvailable,
			}) %>[inputData];
		};
	</script>

	<script src="/node_modules/OLSKRouting/main.js"></script>
	<script type="text/javascript">
		window.OLSKCanonicalFor = function(inputData, params) {
			return OLSKRouting.OLSKRoutingCanonicalPath((<%- JSON.stringify(['OLSKLanguageSwitcherSvelteStubRoute'].reduce(function (map, e) {
				map[e] = OLSKRouteObjectFor(e).OLSKRoutePath;
				return map;
			}, {})) %>)[inputData], params);
		};
	</script>
</head>
<body>

<% if (OLSKSharedActiveRouteConstant !== 'OLSKLanguageSwitcherSvelteStubRoute') { -%>

<%- include('./main.ejs') %>	

<% } -%>

<% if (OLSKSharedActiveRouteConstant === 'OLSKLanguageSwitcherSvelteStubRoute') { -%>

<script src="/<%= OLSKSharedPageControllerSlug %>/__compiled/ui-behaviour.js"></script>

<% } -%>

</body>
</html>
